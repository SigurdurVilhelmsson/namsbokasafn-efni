<m:mrow>
<m:mn>1</m:mn><m:mspace width="0.2em"/><m:mtext>mol</m:mtext><m:mtext>/</m:mtext><m:mn>342.3</m:mn><m:mspace width="0.2em"/><m:mtext>g</m:mtext></m:mrow>
