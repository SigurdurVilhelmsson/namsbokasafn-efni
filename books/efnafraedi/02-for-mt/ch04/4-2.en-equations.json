    "EQ:150": "{{{\\text{Fe}}}^{{2+}}(aq)+{{\\text{Ce}}}^{{4+}}(aq)\\,\\longrightarrow \\,{{\\text{Fe}}}^{{3+}}(aq)+{{\\text{Ce}}}^{{3+}}(aq)}",
    "EQ:151": "{\\text{HBrO}(aq)\\,\\longrightarrow \\,{{\\text{Br}}}^{\\text{−}}(aq)+{\\text{O}}_{2}(g)\\,\\text{(in acid)}}",
    "EQ:152": "{{{\\text{Sn}}}^{{2+}}(aq)+2{{\\text{Cu}}}^{{2+}}(aq)\\,\\longrightarrow \\,{{\\text{Sn}}}^{{4+}}(aq)+2{{\\text{Cu}}}^{\\text{+}}(aq);}",
    "EQ:153": "{{\\text{H}}_{2}\\text{S}(g)+{{\\text{Hg}}}_{2}^{{2+}}(aq)+2{\\text{H}}_{2}\\text{O}(l)\\,\\longrightarrow \\,2\\text{Hg}(l)+\\text{S}(s)+2{\\text{H}}_{3}{\\text{O}}^{\\text{+}}(aq);}",
    "EQ:154": "{5{{\\text{CN}}}^{\\text{−}}(aq)+2{{\\text{ClO}}}_{2}(aq)+3{\\text{H}}_{2}\\text{O}(l)\\,\\longrightarrow \\,5{{\\text{CNO}}}^{\\text{−}}(aq)+2{{\\text{Cl}}}^{\\text{−}}(aq)+2{\\text{H}}_{3}{\\text{O}}^{\\text{+}}(aq);}",
    "EQ:155": "{{{\\text{Fe}}}^{{2+}}(aq)+{{\\text{Ce}}}^{{4+}}(aq)\\,\\longrightarrow \\,{{\\text{Fe}}}^{{3+}}(aq)+{{\\text{Ce}}}^{{3+}}(aq);}",
    "EQ:156": "{2\\text{HBrO}(aq)+2{\\text{H}}_{2}\\text{O}(l)\\,\\longrightarrow \\,2{\\text{H}}_{3}{\\text{O}}^{\\text{+}}(aq)+2{{\\text{Br}}}^{\\text{−}}(aq)+{\\text{O}}_{2}(g)}",
    "EQ:157": "{\\text{Zn}(s)+{{\\text{NO}}}_{3}^{\\text{−}}(aq)\\,\\longrightarrow \\,{{\\text{Zn}}}^{{2+}}(aq)+{\\text{N}}_{2}(g)\\,\\text{(in acid)}}",
    "EQ:158": "{\\text{Zn}(s)+{{\\text{NO}}}_{3}^{\\text{−}}(aq)\\,\\longrightarrow \\,{{\\text{Zn}}}^{{2+}}(aq)+{{\\text{NH}}}_{3}(aq)\\,\\text{(in base)}}",
    "EQ:159": "{\\text{CuS}(s)+{{\\text{NO}}}_{3}^{\\text{−}}(aq)\\,\\longrightarrow \\,{{\\text{Cu}}}^{{2+}}(aq)+\\text{S}(s)+\\text{NO}(g)\\,\\text{(in acid)}}",
    "EQ:160": "{{{\\text{NH}}}_{3}(aq)+{\\text{O}}_{2}(g)\\,\\longrightarrow \\,{{\\text{NO}}}_{2}(g)\\,\\text{(gas phase)}}",
    "EQ:161": "{{\\text{H}}_{2}{\\text{O}}_{2}(aq)+{{\\text{MnO}}}_{4}^{\\text{−}}(aq)\\,\\longrightarrow \\,{{\\text{Mn}}}^{{2+}}(aq)+{\\text{O}}_{2}(g)\\,\\text{(in acid)}}",
    "EQ:162": "{{{\\text{NO}}}_{2}(g)\\,\\longrightarrow \\,{{\\text{NO}}}_{3}^{\\text{−}}(aq)+{{\\text{NO}}}_{2}^{\\text{−}}(aq)\\,\\text{(in base)}}",
    "EQ:163": "{{{\\text{Fe}}}^{{3+}}(aq)+{\\text{I}}^{\\text{−}}(aq)\\,\\longrightarrow \\,{{\\text{Fe}}}^{{2+}}(aq)+{\\text{I}}_{2}(aq)}",
    "EQ:164": "{{{\\text{MnO}}}_{4}^{\\text{−}}(aq)+{{\\text{NO}}}_{2}^{\\text{−}}(aq)\\,\\longrightarrow \\,{{\\text{MnO}}}_{2}(s)+{{\\text{NO}}}_{3}^{\\text{−}}(aq)\\,\\text{(in base)}}",
    "EQ:165": "{{{\\text{MnO}}}_{4}^{{2−}}(aq)\\,\\longrightarrow \\,{{\\text{MnO}}}_{4}^{\\text{−}}(aq)+{{\\text{MnO}}}_{2}(s)\\,\\text{(in base)}}",
    "EQ:166": "{{{\\text{Br}}}_{2}(l)+{{\\text{SO}}}_{2}(g)\\,\\longrightarrow \\,{{\\text{Br}}}^{\\text{−}}(aq)+{{\\text{SO}}}_{4}^{{2−}}(aq)\\,\\text{(in acid)}}",
    "EQ:167": "{2{{\\text{MnO}}}_{4}^{\\text{−}}(aq)+{{\\text{3NO}}}_{2}^{\\text{−}}(aq)+{\\text{H}}_{2}\\text{O}(l)\\,\\longrightarrow \\,2{{\\text{MnO}}}_{2}(s)+3{{\\text{NO}}}_{3}^{\\text{−}}(aq)+2{{\\text{OH}}}^{\\text{−}}(aq);}",
    "EQ:168": "{3{{\\text{MnO}}}_{4}^{{2−}}(aq)+2{\\text{H}}_{2}\\text{O}(l)\\,\\longrightarrow \\,2{{\\text{MnO}}}_{4}^{\\text{−}}(aq)+4{{\\text{OH}}}^{\\text{−}}(aq)+{{\\text{MnO}}}_{2}(s)\\,\\text{(in base)};}",
    "EQ:169": "{{{\\text{Br}}}_{2}(l)+{{\\text{SO}}}_{2}(g)+2{\\text{H}}_{2}\\text{O}(l)\\,\\longrightarrow \\,4{\\text{H}}^{\\text{+}}(aq)+2{{\\text{Br}}}^{\\text{−}}(aq)+{{\\text{SO}}}_{4}^{{2−}}(aq)}"
