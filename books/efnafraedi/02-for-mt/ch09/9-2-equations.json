  "title": "Relating Pressure, Volume, Amount, and Temperature: The Ideal Gas Law",
  "equations": {
    "EQ:1": {
      "latex": "{P∝T\\,\\text{or}\\,P=\\text{constant}\\,\\times \\,T\\,\\text{or}\\,P=k\\,\\times \\,T}",
      "id": "fs-idp58663824"
    },
    "EQ:2": {
      "latex": "{\\frac{{{P}_{1}}}{{{T}_{1}}}\\,=\\,\\frac{{{P}_{2}}}{{{T}_{2}}}\\,\\text{which means that}\\,\\frac{{360\\,\\text{kPa}}}{{297\\,\\text{K}}}\\,=\\,\\frac{{{P}_{2}}}{{323\\,\\text{K}}}}",
      "id": "fs-idp253098544"
    },
    "EQ:3": {
      "latex": "{V\\,\\text{α}\\,T\\,\\text{or}\\,V=\\text{constant}\\text{·}T\\,\\text{or}\\,V=k\\text{·}T\\,\\text{or}\\,{V}_{1}\\text{/}{T}_{1}={V}_{2}\\text{/}{T}_{2}}",
      "id": "fs-idm59931152"
    },
    "EQ:4": {
