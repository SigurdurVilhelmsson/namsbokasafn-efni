    },
    "EQ:2": {
      "latex": "\\text{rate of decomposition of}\\,{\\text{H}}_{2}{\\text{O}}_{2}=-\\,\\frac{{\\text{change in concentration of reactant}}}{{\\text{time interval}}}=-\\,\\frac{{{{[{\\text{H}2{\\text{O}}_{2}}]}}{{t}_{2}}\\,-\\,{{[{\\text{H}2{\\text{O}}_{2}}]}}{{t}_{1}}}}{{{t}_{2}\\,-\\,{t}_{1}}}=-\\,\\frac{{\\text{Δ}{[{{\\text{H}}_{2}{\\text{O}}_{2}}]}}}{{\\text{Δ}t}}",
      "id": "fs-idm23827472"
    },
    "EQ:3": {
      "latex": "{\\frac{{-\\text{Δ}[{\\text{H}}_{2}{\\text{O}}_{2}]}}{{\\text{Δ}t}}\\,=\\,\\frac{{−(\\text{0.500 mol/L}-\\text{1.000 mol/L})}}{{(\\text{6.00 h}-\\text{0.00 h})}}\\,=0.0833 mol\\,{{\\text{L}}}^{{−1}}\\,{{\\text{h}}}^{{−1}}}",
      "id": "fs-idm14828496"
    },
    "EQ:4": {
      "latex": "{\\frac{{-\\text{Δ}[{\\text{H}}_{2}{\\text{O}}_{2}]}}{{\\text{Δ}t}}\\,=\\,\\frac{{-(0.0625\\,\\text{mol/L}-0.125\\,\\text{mol/L})}}{{(24.00\\,\\text{h}-18.00\\,\\text{h})}}\\,=0.010\\,\\text{mol}\\,{\\text{L}}^{{−1}}\\,{\\text{h}}^{{−1}}}",
      "id": "fs-idp151295040"
